{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "module": "esnext",
    "outDir": "packages/lib",
    "lib": ["esnext"],
    "jsx": "react-native",
    "pretty": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "declaration": true,
    "removeComments": false,
    "isolatedModules": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmit": false
  },
  "extends": "expo/tsconfig.base"
}
